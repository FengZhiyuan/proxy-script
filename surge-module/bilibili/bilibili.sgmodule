#!name=bilibili
#!desc=当前支持首页去列表区广告

[MITM]
hostname = %APPEND% app.bilibili.com, grpc.biliapi.net

[Script]
bilibili-remove-ad=type=http-response,pattern=^https?://.*\.bilibili\.c(n|om)/.*$,requires-body=1,max-size=0,debug=1,script-path=https://raw.githubusercontent.com/FengZhiyuan/proxy-script/main/surge-module/bilibili/bilibili-ad.js,script-update-interval=0



